@model List<Quintessence.QService.QueryModel.Sec.UserView>

<table id="UsersInRoleTable">
    <thead>
        <tr>
            <th style="width: 80%;">Name</th>
            <th style="width: 20%;">Username</th>
            <th style="width: 72px;">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FullName</td>
                <td>@user.UserName</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>

<br />
<br />
<div class="right-text">
    <a href="javascript:void(0)" data-function="RefreshRoleUsers" class="button">Refresh</a>
    <a href="javascript:void(0)" data-function="AddRoleUser" class="button">Add</a>
</div>
