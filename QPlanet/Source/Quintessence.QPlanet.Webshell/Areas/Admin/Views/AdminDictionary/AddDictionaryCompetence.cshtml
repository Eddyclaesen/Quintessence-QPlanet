@model Quintessence.QPlanet.ViewModel.Dim.CreateNewDictionaryCompetenceModel
@using (Html.BeginForm("AddDictionaryCompetence", "AdminDictionary", FormMethod.Post, new { id = "AddDictionaryCompetenceForm" }))
{
    @Html.HiddenFor(m => m.DictionaryClusterId)
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.DictionaryName)</div>
        <div>@Html.DisplayTextFor(m => m.DictionaryName)</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.DictionaryClusterName)</div>
        <div>@Html.DisplayTextFor(m => m.DictionaryClusterName)</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.Name)</div>
        <div>@Html.EditorFor(m => m.Name)</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.Description)</div>
        <div>@Html.TextAreaFor(m => m.Description, 4, 40, null)</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.LanguageId)</div>
        <div>@Html.DropDownListFor(m => m.LanguageId, Model.CreateLanguageList())</div>
    </div>
    <div class="row"></div>
    
    <div class="right-text">
        <a href="javascript:void();" data-function="SaveDictionaryCompetence" class="button">Save</a>
    </div>
}