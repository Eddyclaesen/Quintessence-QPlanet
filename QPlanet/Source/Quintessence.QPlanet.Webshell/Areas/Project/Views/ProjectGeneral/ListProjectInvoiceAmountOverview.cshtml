@model Quintessence.QPlanet.Webshell.Areas.Project.Models.ProjectGeneral.ListProjectInvoiceAmountEntriesModel

<table id="ProjectInvoiceAmountOverviewTable">
    <thead>
        <tr>
            <th>Fiscal year</th>
            <th>Invoice amount</th>
        </tr>
    </thead>
    <tbody>
       @for (int i = 0; i < Model.InvoiceAmountDictionary.Count; i++)
    {
        <tr>
            <td>@string.Format("{0}/{1}", Model.InvoiceAmountDictionary.ElementAt(i).Key, Model.InvoiceAmountDictionary.ElementAt(i).Key + 1)</td>
            <td class="right-text">@Model.InvoiceAmountDictionary.ElementAt(i).Value.ToString("C")</td>
        </tr>
    }  
    </tbody>
   
</table>
