@model List<Quintessence.QService.QueryModel.Prm.RecentProjectView>
@if (Model != null)
{
    for (int i = 0; i < Model.Count(); i++)
    {
        if (i % 2 == 0)
        {
        <tr class="even">
            @Html.DisplayFor(m => m[i])
        </tr>
        }
        else
        {
        <tr class="odd">
            @Html.DisplayFor(m => m[i])
        </tr>
        }
    }
}
else
{
    <tr>
        <td colspan="3">No projects found.</td>
    </tr>
}