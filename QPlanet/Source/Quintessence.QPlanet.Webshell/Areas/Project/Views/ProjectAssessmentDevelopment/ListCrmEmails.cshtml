@model List<Quintessence.QPlanet.Webshell.Areas.Project.Models.ProjectAssessmentDevelopment.SelectCrmEmailModel>
<table id="SelectCrmEmailsDataTable" class="dataTable">
    <thead>
        <tr>
            <th>Selected</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Direct</th>
            <th>Mobile</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Html.EditorFor(m => m[i].IsSelected)</td>
                <td>
                    @*@Html.HiddenFor(m => m[i].IsSelected)*@
                    @Html.HiddenFor(m => m[i].Id)
                    @*@Html.HiddenFor(m => m[i].ContactId)
                    @Html.HiddenFor(m => m[i].FirstName)
                    @Html.HiddenFor(m => m[i].LastName)
                    @Html.HiddenFor(m => m[i].Email)
                    @Html.HiddenFor(m => m[i].DirectPhone)
                    @Html.HiddenFor(m => m[i].MobilePhone)*@
                    @Html.DisplayFor(m => m[i].FirstName)
                </td>
                <td>@Html.DisplayFor(m => m[i].LastName)</td>
                <td>@Html.DisplayFor(m => m[i].Email)</td>
                <td>@Html.DisplayFor(m => m[i].DirectPhone)</td>
                <td>@Html.DisplayFor(m => m[i].MobilePhone)</td>
            </tr>
        }
    </tbody>
</table>
