@model Quintessence.QPlanet.ViewModel.Prm.CreateCulturalFitContactRequestModel
@using (Html.BeginForm("CreateCulturalFitContactRequest", "ProjectAssessmentDevelopment", FormMethod.Post, new { id = "CreateCulturalFitContactRequestForm" }))
{
    @Html.HiddenFor(m => m.ContactId)
    @Html.HiddenFor(m => m.ProjectId)
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.CrmEmailId)</div>
        <div>@Html.DropDownListFor(m => m.CrmEmailId, Model.CreateContactPersonDropDownList(Model.CrmEmailId))</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.TheoremListRequestTypeId)</div>
        <div>@Html.DropDownListFor(m => m.TheoremListRequestTypeId, Model.CreateTheoremListRequestTypeSelectListItems(Model.TheoremListRequestTypeId))</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.Deadline)</div>
        <div>@Html.EditorFor(m => m.Deadline, "DateEditorTemplate")</div>
    </div>
    <div class="row">
        <div>@Html.DisplayNameFor(m => m.Description)</div>
        <div>@Html.EditorFor(m => m.Description)</div>
    </div>
    <br />
    <br />
    <div class="right-text">
        <a href="javascript:void(0);" class="button" id="CreateCulturalFitContactRequestSubmitButton">Save</a>
    </div>

}

