@model List<Quintessence.QService.QueryModel.Prm.ProjectTypeCategoryUnitPriceView>
@if (Model.Count > 0)
{
    <table class="dataTable">
        @foreach (var projectTypeCategoryUnitPrice in Model)
        {
            <tr>
                <td>@projectTypeCategoryUnitPrice.ProjectTypeCategoryLevel.Name</td>
                <td><a href="javascript:void(0);" title="Select unit price" data-function="CopyUnitPrice" data-project-type-category-id="@projectTypeCategoryUnitPrice.ProjectTypeCategoryId" data-value="@projectTypeCategoryUnitPrice.UnitPrice">@projectTypeCategoryUnitPrice.UnitPrice.ToString("C")</a></td>
            </tr>
        }
    </table>
}
else
{
    <p class="center-text">...No unit prices avaiable...</p>
}

