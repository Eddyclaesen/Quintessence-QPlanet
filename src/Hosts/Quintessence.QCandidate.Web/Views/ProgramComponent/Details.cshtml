@using Quintessence.QCandidate.Controllers
@model Quintessence.QCandidate.Contracts.Responses.ProgramComponentDto
@{
    var pdfUrl = $"{Url.Action(nameof(SimulationCombinationsController.GetPdf), "SimulationCombinations", new { simulationCombinationId = Model.SimulationCombinationId })}#toolbar=0";
}

@if(ViewBag.CanShowPdf)
{
    <iframe src="@pdfUrl" width="100%" height="500px">
    </iframe>
}